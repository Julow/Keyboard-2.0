<?xml version="1.0" encoding="utf-8"?>
<!-- Adapted from standard NF Z71-300 -->
<!-- https://norme-azerty.fr/ -->
<keyboard>
  <row>
    <key k0="a" k2="1" k4="esc"/>
    <key k0="z" k2="2" k4="~" k6="&amp;"/>
    <key k0="e" k2="3" k4="\#" k6="&quot;"/>
    <key k0="r" k2="4" k6="'"/>
    <key k0="t" k2="5" k4=")" k6="("/>
    <key k0="y" k2="6" k4="|" k6="-"/>
    <key k0="u" k2="7" k4="`"/>
    <key k0="i" k2="8" k4="\\" k6="_"/>
    <key k0="o" k2="9" k4="f11_placeholder" k6="\@"/>
    <key k0="p" k2="0" k6="f12_placeholder"/>
  </row>
  <row>
    <key k0="q" k2="tab"/>
    <key k0="s" k6="loc ß"/>
    <key k0="d" k6="acute" k8="grave"/>
    <key k0="f" k4="}" k6="{"/>
    <key k0="g" k4="]" k6="["/>
    <key k0="h" k4="+" k6="="/>
    <key k0="j" k2="circumflex" k6="^" k8="two_dots_above"/>
    <key k0="k" k2="€" k6="$"/>
    <key k0="l" k2="%"/>
    <key k0="m" k6="*"/>
  </row>
  <row>
    <key width="2.0" k0="shift" k2="loc capslock"/>
    <key k0="w" k4="&gt;" k6="&lt;"/>
    <key k0="x"/>
    <key k0="c" k4="\?" k6="," k8="cedille"/>
    <key k0="v" k4="." k6=";"/>
    <key k0="b" k4="/" k6=":"/>
    <key k0="n" k2="§" k4="!" k8="loc tilde"/>
    <key width="2.0" k0="backspace" k2="delete"/>
  </row>
</keyboard>
